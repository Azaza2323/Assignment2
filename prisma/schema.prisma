
generator client {
  provider = "prisma-client-js"
}
model authors {
  author_id  Int          @id @default(autoincrement())
  name       String?
  surname    String?
  birthday   DateTime?
}

model books{
  book_id      Int          @id @default(autoincrement())
  title        String?
  publish_year DateTime?
  page_count   Int?
  price        Float?
}

model genres {
  genre_id   Int         @id @default(autoincrement())
  genre_name String?

}
datasource db {
  provider = "mysql"
  url      = "mysql://root:123123@localhost:3306/library"
}
